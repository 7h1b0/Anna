{
  "name": "anna",
  "version": "3.1.2",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "dependencies": {
    "ajv": "^6.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cron": "^1.6.0",
    "express": "^4.16.4",
    "knex": "^0.16.2",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "env-cmd": "^8.0.2",
    "eslint": "^5.10.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "lolex": "^3.0.0",
    "nodemon": "^1.18.8",
    "pkg": "^4.3.5",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "eslint": "eslint --max-warnings 0 src migrations seeds",
    "test": "jest src --watch --notify --runInBand",
    "coverage": "jest src --coverage --ci --runInBand --silent",
    "start": "env-cmd --no-override .env.local nodemon --watch src --exec babel-node index.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "build": "webpack",
    "build:package": "npm run build && pkg -t node10-linux-armv7 ./dist/index.js --public --no-bytecode -o anna-armv7"
  }
}
