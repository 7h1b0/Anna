{
  "name": "anna",
  "version": "3.2.1",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "dependencies": {
    "ajv": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cron-parser": "^2.11.0",
    "express": "^4.17.0",
    "knex": "^0.16.5",
    "mysql": "^2.17.1",
    "node-fetch": "^2.6.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/jest-matcher-utils": "^21.0.2",
    "@types/knex": "^0.16.1",
    "@types/lolex": "^3.1.1",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.3.4",
    "@types/uuid": "^3.4.4",
    "clean-webpack-plugin": "^2.0.2",
    "env-cmd": "^9.0.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "lolex": "^4.0.1",
    "nodemon": "^1.19.0",
    "pkg": "^4.3.8",
    "prettier": "^1.17.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "test": "jest src --watch --notify --runInBand",
    "coverage": "jest src --coverage --ci --runInBand --silent",
    "start": "env-cmd --no-override .env.local nodemon --watch src --exec babel-node index.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "build": "webpack",
    "build:package": "npm run build && pkg -t node10-linux-armv7 ./dist/index.js --public --no-bytecode -o anna-armv7",
    "tsc": "tsc --noEmit",
    "prettier:check": "prettier --list-different \"src/**/*.ts\""
  }
}
