{
  "name": "anna",
  "version": "3.2.1",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "dependencies": {
    "ajv": "^6.9.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cron-parser": "^2.7.3",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.6",
    "@types/jest-matcher-utils": "^21.0.2",
    "@types/knex": "^0.15.2",
    "@types/lolex": "0.0.0",
    "@types/node": "^11.9.4",
    "@types/node-fetch": "^2.1.6",
    "@types/uuid": "^3.4.4",
    "clean-webpack-plugin": "^1.0.0",
    "env-cmd": "^8.0.2",
    "husky": "^1.2.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "lolex": "^3.1.0",
    "nodemon": "^1.18.10",
    "pkg": "^4.3.7",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "test": "jest src --watch --notify --runInBand",
    "coverage": "jest src --coverage --ci --runInBand --silent",
    "start": "env-cmd --no-override .env.local nodemon --watch src --exec babel-node index.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "build": "webpack",
    "build:package": "npm run build && pkg -t node10-linux-armv7 ./dist/index.js --public --no-bytecode -o anna-armv7",
    "tsc": "tsc --noEmit",
    "prettier:check": "prettier --list-different \"src/**/*.ts\""
  }
}
