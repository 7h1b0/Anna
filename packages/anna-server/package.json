{
  "name": "anna-server",
  "version": "3.5.0",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@swc/jest": "^0.2.17",
    "ajv": "^8.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cron-parser": "^4.2.1",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "knex": "^0.19.3",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.16.8",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.12",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.10.2",
    "@types/node-fetch": "^2.5.10",
    "@types/sinonjs__fake-timers": "^6.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.9",
    "pkg": "^5.3.3",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' index.ts",
    "db:migrate": "knex migrate:latest",
    "db:make": "knex migrate:make",
    "db:rollback": "knex migrate:rollback",
    "build:ts": "tsc",
    "build": "npm run build:ts && pkg -t node16.8-linuxstatic-armv7 ./dist/index.js --public --no-bytecode -o anna-armv7",
    "build:test": "npm run build && pkg -t node16.8-linuxstatic-x64 ./dist/index.js --public --no-bytecode -o anna-x64",
    "type:check": "tsc --noEmit"
  }
}
