{
  "name": "anna",
  "version": "3.1.0",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "dependencies": {
    "ajv": "^6.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cron": "^1.5.0",
    "express": "^4.16.4",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "node-fetch": "^2.2.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "nodemon": "^1.18.6",
    "pkg": "^4.3.4",
    "prettier": "^1.15.1",
    "supertest": "^3.3.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "eslint": "eslint --max-warnings 0 src migrations seeds",
    "test": "jest src --watch --notify --runInBand",
    "coverage": "jest src --coverage --ci --runInBand --silent",
    "start": "nodemon --watch src --exec babel-node index.js",
    "db:migrate": "knex migrate:latest --knexfile src/knexfile.js",
    "db:rollback": "knex migrate:rollback --knexfile src/knexfile.js",
    "db:seed": "knex seed:run --knexfile src/knexfile.js",
    "build": "webpack --config webpack.config.js",
    "build:package": "npm run build && pkg -t node10-linux-armv7 ./dist/index.js --public --no-bytecode -o anna"
  }
}
