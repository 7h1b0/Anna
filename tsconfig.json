{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true
  }
}
