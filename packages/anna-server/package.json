{
  "name": "anna-server",
  "version": "3.4.2",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cron-parser": "^2.13.0",
    "express": "^4.17.1",
    "knex": "^0.19.3",
    "mysql": "^2.17.1",
    "node-fetch": "^2.6.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/node": "^7.6.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/jest-matcher-utils": "^21.0.2",
    "@types/knex": "^0.16.1",
    "@types/lolex": "^3.1.1",
    "@types/node": "^12.7.8",
    "@types/node-fetch": "^2.5.2",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lolex": "^4.2.0",
    "nodemon": "^1.19.3",
    "pkg": "^4.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "test": "jest src --watch --notify --runInBand",
    "test:e2e": "jest e2e",
    "coverage": "jest src --coverage --ci --runInBand --silent",
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' -r tsconfig-paths/register index.ts",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "build": "webpack",
    "build:release": "npm run build && pkg -t node10-linux-armv7 ./dist/index.js --public --no-bytecode -o anna-armv7",
    "build:test": "npm run build && pkg -t node10-linux-x64 ./dist/index.js --public --no-bytecode -o anna-x64",
    "lint:check": "eslint --max-warnings 0 '*/**/*.ts'",
    "type:check": "tsc --noEmit",
    "format:check": "prettier --list-different \"src/**/*.ts\""
  }
}
