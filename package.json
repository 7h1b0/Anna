{
  "name": "anna",
  "version": "3.1.0",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "dependencies": {
    "ajv": "^6.5.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cron": "^1.4.1",
    "express": "^4.16.3",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "supertest": "^3.3.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "eslint": "eslint --max-warnings 0 src migrations seeds",
    "test": "jest src --watch --notify --runInBand",
    "coverage": "jest src --coverage --ci --runInBand --silent",
    "start": "nodemon --watch src --exec babel-node index.js",
    "precommit": "lint-staged",
    "postversion": "git push --follow-tags",
    "db:migrate": "knex migrate:latest --knexfile src/knexfile.js",
    "db:rollback": "knex migrate:rollback --knexfile src/knexfile.js",
    "db:seed": "knex seed:run --knexfile src/knexfile.js",
    "build": "webpack --config webpack.config.js"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --single-quote --trailing-comma all --print-width 80 --write",
      "git add"
    ]
  },
  "jest": {
    "testURL": "http://localhost"
  }
}
