{
  "name": "anna-server",
  "version": "3.5.0",
  "description": "REST api for home automation",
  "main": "src/index.js",
  "author": "7h1b0",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@bokub/linky": "^0.2.2",
    "ajv": "^6.12.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cron-parser": "^2.16.3",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "knex": "^0.19.3",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.7",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/sinonjs__fake-timers": "^6.0.1",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.4",
    "pkg": "^4.4.9",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' -r tsconfig-paths/register index.ts",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "build": "webpack",
    "build:release": "npm run build && pkg -t node10.17-linux-armv7 ./dist/index.js --public --no-bytecode -o anna-armv7",
    "build:test": "npm run build && pkg -t node10-linux-x64 ./dist/index.js --public --no-bytecode -o anna-x64",
    "type:check": "tsc --noEmit"
  }
}
